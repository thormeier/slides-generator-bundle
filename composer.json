{
    "name": "thormeier/slides-generator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for automated setup of slides",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Thormeier",
            "email": "pascal.thormeier@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": { "Thormeier\\SlidesGeneratorBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Thormeier\\SlidesGeneratorBundle\\Tests\\": "tests/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.6,<=3.1",
        "kzykhys/git": "v0.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "symfony/console": "v3.1.*",
        "symfony/finder": "*"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
